{
  "name": "takeme",
  "version": "0.11.1",
  "description": "A simple js routing solution",
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "demo": "node fuse production",
    "demo:client": "node fuse client",
    "demo:server": "node fuse server",
    "build": "tsc -p .",
    "unittest": "mocha ./lib/tests/**/*.js",
    "test": "npm run build && npm run unittest",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags && npm publish",
    "start": "npm run build -- -w & npm run unittest -- --watch & npm run demo:live",
    "publishdemo": "npm run demo && ts-node ./src/scripts/publishdemo.ts"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.6.3",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "core-js": "3.1.4",
    "csstips": "1.1.0",
    "express": "^4.16.2",
    "fuse-box": "3.7.1",
    "fuse-hmr": "0.2.0",
    "gh-pages": "2.0.1",
    "mobx": "^5.11.0",
    "mobx-react": "^6.1.1",
    "mocha": "6.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "ts-node": "8.3.0",
    "typescript": "3.5.3",
    "typestyle": "2.0.3",
    "uglify-js": "^3.6.0"
  }
}
